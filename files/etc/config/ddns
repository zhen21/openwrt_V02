
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'myddns_ipv4'
	option interface 'wan'
	option ip_source 'network'
	option ip_network 'wan'
	option service_name 'aliyun.com'
	option enabled '1'
	option username 'LTAI4FiYrDgqeDfcPxVPFbuj'
	option password 'XPAy3vCZMuzRowTnE7W0NpgS0Lb7So'
	option check_interval '5'
	option lookup_host 'fangzhen.vip'
	option domain 'www.fangzhen.vip'

config service 'myddns_ipv6'
	option ip_source 'network'
	option service_name 'aliyun.com'
	option lookup_host 'fangzhen.vip'
	option username 'LTAI4FiYrDgqeDfcPxVPFbuj'
	option password 'XPAy3vCZMuzRowTnE7W0NpgS0Lb7So'
	option interface 'lan'
	option ip_network 'lan'
	option check_interval '5'
	option domain '@fangzhen.vip'
	option enabled '0'

